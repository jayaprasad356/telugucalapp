<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.PanchnagamListFragment"
    android:background="#F6F3F3"
    >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Text"
            android:textColor="@color/colorBlack"
            android:gravity="center"
            android:padding="@dimen/_5sdp"
            android:textSize="@dimen/_12sdp"
            />







            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/_10sdp"
                android:gravity="center"
                android:weightSum="3"

                >

            <LinearLayout
                android:layout_weight="1"
                android:id="@+id/llGowri"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp"
                >

                <RelativeLayout
                    android:layout_gravity="center"
                    android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:background="@drawable/rounded_bar"
                >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />



                </RelativeLayout>
            <TextView
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Gowri"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:padding="@dimen/_2sdp"
                />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp"

                >

                <RelativeLayout
                    android:layout_gravity="center"

                    android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:background="@drawable/rounded_bar"
                >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />



                </RelativeLayout>
            <TextView
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Hora chakram"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:padding="@dimen/_2sdp"
                />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:id="@+id/llThidhi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp"

                >

                <RelativeLayout
                    android:layout_gravity="center"

                    android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:background="@drawable/rounded_bar"
                >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />



                </RelativeLayout>
            <TextView
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Thidhi"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:padding="@dimen/_2sdp"
                />

            </LinearLayout>






    </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/_10sdp"
                android:gravity="center"
                android:weightSum="3"
                >


            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp"

                >

                <RelativeLayout
                android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:background="@drawable/rounded_bar"
                    android:layout_gravity="center"

                    >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />



                </RelativeLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Karanam"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:padding="@dimen/_2sdp"
                android:layout_gravity="center"
                />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:id="@+id/llRahukalam"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp"

                >

                <RelativeLayout
                android:layout_width="@dimen/_60sdp"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_60sdp"
                android:background="@drawable/rounded_bar"
                >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent" />



                </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Rahukalam"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:padding="@dimen/_2sdp"
                android:layout_gravity="center"

                />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp"

                >

                <RelativeLayout
                    android:layout_gravity="center"
                android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:background="@drawable/rounded_bar"
                >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>



                </RelativeLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Yogam"
                android:gravity="center"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:padding="@dimen/_2sdp"
                android:layout_gravity="center"

                />

            </LinearLayout>





    </LinearLayout>


    </LinearLayout>







</RelativeLayout>